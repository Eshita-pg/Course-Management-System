<%- include("studentDashHeader") -%>
<br><br>
<div class="col offset-3"id="main">
    <%  data.forEach(function(course){ %>
    <div class="card" style=" max-width:800px;">
     <div class="card-body" id="cardBg">
       <h5 class="card-title"><%= course.course_id %></h5>
       <h6 class="card-subtitle mb-2 text-muted"><%= course.course_name %> (Credits <%= course.credits %>)</h6>
       <p class="card-text"><%= course.course_description %> </p>
     </div>
   </div>
   <%  }); %>

   
</div>


<%- include("studentDashFooter") -%>